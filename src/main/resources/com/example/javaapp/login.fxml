<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="223.0" prefWidth="528.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javaapp.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
   <Pane prefHeight="247.0" prefWidth="754.0" />
   <TextField fx:id="EnterGym" alignment="CENTER" prefHeight="56.0" prefWidth="189.0" text="ENTER GYM ID">
      <font>
         <Font size="19.0" />
      </font>
   </TextField>
   <Button fx:id="Enter" mnemonicParsing="false" onAction="#login" prefHeight="56.0" prefWidth="196.0" text="Enter">
      <font>
         <Font size="24.0" />
      </font>
   </Button>
</VBox>
