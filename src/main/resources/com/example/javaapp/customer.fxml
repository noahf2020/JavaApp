<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="314.0" prefWidth="680.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javaapp.CustomController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
   <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="248.0" prefWidth="674.0">
      <children>
         <Pane layoutX="93.0" layoutY="46.0" prefHeight="258.0" prefWidth="605.0">
            <children>
               <Button fx:id="LOGOUT" layoutX="142.0" layoutY="140.0" mnemonicParsing="false" onAction="#logout" prefHeight="54.0" prefWidth="161.0" text="LOG OUT">
                  <font>
                     <Font name="System Bold" size="17.0" />
                  </font>
               </Button>
               <Label fx:id="AOV" layoutX="156.0" layoutY="13.0" prefHeight="34.0" prefWidth="134.0" text="Amount of visits ">
                  <font>
                     <Font name="System Bold" size="18.0" />
                  </font>
               </Label>
               <Label fx:id="PFP" layoutX="382.0" layoutY="13.0" prefHeight="33.0" prefWidth="110.0" text="Profile Picture">
                  <font>
                     <Font name="System Bold" size="18.0" />
                  </font>
               </Label>
               <ImageView fitHeight="128.0" fitWidth="175.0" layoutX="404.0" layoutY="41.0" pickOnBounds="true" preserveRatio="true" />
               <Button fx:id="Uploadpicture" layoutX="142.0" layoutY="78.0" mnemonicParsing="false" onAction="#uploadPic" prefHeight="54.0" prefWidth="161.0" text="Upload Picture">
                  <font>
                     <Font name="System Bold" size="17.0" />
                  </font>
               </Button>
               <Label fx:id="Standing" layoutX="-52.0" layoutY="14.0" prefHeight="34.0" prefWidth="134.0" text="Standing ">
                  <font>
                     <Font name="System Bold" size="18.0" />
                  </font>
               </Label>
            </children>
         </Pane>
         <ImageView fitHeight="125.0" fitWidth="171.0" layoutX="10.0" layoutY="97.0" pickOnBounds="true" preserveRatio="true" />
         <Text fx:id="Welcomeback" layoutX="162.0" layoutY="20.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome back" textAlignment="CENTER" wrappingWidth="317.0">
            <font>
               <Font name="System Bold" size="19.0" />
            </font>
         </Text>
      </children>
   </AnchorPane>
</VBox>
